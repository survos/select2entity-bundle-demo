{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">= 8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.7.0",
        "doctrine/orm": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.6",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/asset": "^4.4 || ^5.4 || ^6.0",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/dotenv": "^4.4 || ^5.4 || ^6.0",
        "symfony/expression-language": "^4.4 || ^5.4 || ^6.0",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-client": "^4.4 || ^5.4 || ^6.0",
        "symfony/intl": "^4.4 || ^5.4 || ^6.0",
        "symfony/messenger": "^4.4 || ^5.4 || ^6.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^4.4 || ^5.4 || ^6.0",
        "symfony/property-access": "^4.4 || ^5.4 || ^6.0",
        "symfony/property-info": "^4.4 || ^5.4 || ^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/serializer": "^4.4 || ^5.4 || ^6.0",
        "symfony/translation": "^4.4 || ^5.4 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/validator": "^4.4 || ^5.4 || ^6.0",
        "symfony/web-link": "^4.4 || ^5.4 || ^6.0",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.0",
        "tetranz/select2entity-bundle": "^3.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.4 || ^5.4 || ^6.0",
        "symfony/css-selector": "^4.4 || ^5.4 || ^6.0",
        "symfony/debug-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/maker-bundle": "^1.12",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "^4.4 || ^5.4 || ^6.0",
        "symfony/web-profiler-bundle": "^4.4 || ^5.4 || ^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "admin_lte_bundle": {
            "type": "vcs",
            "url": "git@github.com:tacman/AdminLTEBundle.git"
        },
        "survos_base_bundle": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        }
    }
}
